HAT FAST FUNKTIONIERT NUR DATA KONNTE NICHT ACCESSED WERDEN
const cats: string [] = [];

projects.forEach((proj) => {
  const cat = proj.data.category;
  cats.push(cat);
})

return Array.from(new Set(cats)).map((cat) => {
  return {
    params: { cat },
    props: {
      cat,
      projects: projects.filter((proj) => proj.data.category.includes(cat)),
      },
    };
  });
}

type Props = CollectionEntry<'projects'>;
